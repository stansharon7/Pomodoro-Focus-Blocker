{
    "manifest_version": 3,
    "name": "Pomodoro Focus Blocker",
    "version": "1.0",
    "description": "A Pomodoro timer with website blocking to boost productivity.",
    "permissions": ["tabs", "storage", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
}
}